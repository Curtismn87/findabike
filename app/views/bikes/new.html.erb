<h1> New Bike Page </h1>

<%= form_for [@user, @bike] do |f| %>

  <%= f.label :model %>
  <%= f.text_field :model %>

  <%= f.label :color%>
  <%= f.text_field :color %>

  <%= f.label :year%>
  <%= f.text_field :year %>

  <%= f.label :type%>
  <%= f.text_field :type %>

  <%= f.label :serial_number%>
  <%= f.text_field :serial_number %>

  <%= f.label :purchase_date%>
  <%= f.text_field :purchase_date %>

  <%= f.label :purchase_price%>
  <%= f.text_field :purchase_price %>

  <%= f.label :receipt_document%>
  <%= f.text_field :receipt_document %>

  <%= f.label :receipt_photo_url%>
  <%= f.text_field :receipt_photo_url %>

  <%= f.submit %>
<% end  %>
